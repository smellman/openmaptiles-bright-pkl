extends "extends/tunnel.pkl"

id = "tunnel-trunk-primary-casing"
filter {
    "all"
    new {
        "=="
        "brunnel"
        "tunnel"
    }
    new {
        "in"
        "class"
        "primary"
        "trunk"
    }
}
layout {
    `line-join` = "round"
}
paint {
    `line-color` = "#e9ac77"
    `line-width` {
        base = 1.2
        stops {
            new {
                5
                0.4
            }
            new {
                6
                0.6
            }
            new {
                7
                1.5
            }
            new {
                20
                22
            }
        }
    }
}