extends "extends/transportation.pkl"

id = "cablecar"
minzoom = 13
filter {
    "=="
    "subclass"
    "cable_car"
}
layout {
    `line-cap` = "round"
    visibility = "visible"
}
paint {
    `line-color` = "hsl(0, 0%, 70%)"
    `line-width` {
        base = 1
        stops {
            new {
                11
                1
            }
            new {
                19
                2.5
            }
        }
    }
}