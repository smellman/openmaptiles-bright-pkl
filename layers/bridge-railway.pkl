extends "extends/transportation.pkl"

id = "bridge-railway"
filter {
    "all"
    new {
        "=="
        "brunnel"
        "bridge"
    }
    new {
        "=="
        "class"
        "rail"
    }
}
paint {
    `line-color` = "#bbb"
    `line-width` {
        base = 1.4
        stops {
            new {
                14
                0.4
            }
            new {
                15
                0.75
            }
            new {
                20
                2
            }
        }
    }
}