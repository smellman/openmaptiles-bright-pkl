extends "extends/transportation.pkl"

id = "tunnel-secondary-tertiary"
filter {
    "all"
    new {
        "=="
        "brunnel"
        "tunnel"
    }
    new {
        "in"
        "class"
        "secondary"
        "tertiary"
    }
}
layout {
    `line-join` = "round"
}
paint {
    `line-color` = "#fff4c6"
    `line-width` {
        base = 1.2
        stops {
            new {
                6.5
                0
            }
            new {
                7
                0.5
            }
            new {
                20
                10
            }
        }
    }
}