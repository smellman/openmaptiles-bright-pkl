extends "extends/transportation.pkl"

id = "cablecar-dash"
minzoom = 13
filter {
    "=="
    "subclass"
    "cable_car"
}
layout {
    `line-cap` = "round"
    visibility = "visible"
}
paint {
    `line-color` = "hsl(0, 0%, 70%)"
    `line-dasharray` {
        2
        3
    }
    `line-width` {
        base = 1
        stops {
            new {
                11
                3
            }
            new {
                19
                5.5
            }
        }
    }
}