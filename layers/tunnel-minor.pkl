extends "extends/transportation-line.pkl"

id = "tunnel-minor"
filter {
    "all"
    new {
        "=="
        "brunnel"
        "tunnel"
    }
    new {
        "=="
        "class"
        "minor_road"
    }
}
layout {
    `line-join` = "round"
}
paint {
    `line-color` = "#fff"
    `line-opacity` = 1
    `line-width` {
        base = 1.2
        stops {
            new {
                13.5
                0
            }
            new {
                14
                2.5
            }
            new {
                20
                11.5
            }
        }
    }
}