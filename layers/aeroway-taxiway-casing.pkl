extends "extends/aeroway.pkl"

id = "aeroway-taxiway-casing"
minzoom = 12
filter {
    "all"
    new {
        "in"
        "class"
        "taxiway"
    }
}
layout {
    `line-cap` = "round"
    `line-join` = "round"
    visibility = "visible"
}
paint {
    `line-color` = "rgba(153, 153, 153, 1)"
    `line-opacity` = 1
    `line-width` {
        base = 1.5
        stops {
            new {
                11
                2
            }
            new {
                17
                12
            }
        }
    }
}