extends "extends/landcover.pkl"

filter {
    "=="
    "class"
    "wood"
}
paint {
    `fill-antialias` {
        base = 1
        stops {
            new {
                0
                "false"
            }
            new {
                9
                "true"
            }
        }
    }
    `fill-color` = "#6a4"
    `fill-opacity` = 0.1
    `fill-outline-color` = "hsla(0, 0%, 0%, 0.03)"
}