extends "extends/transportation-line.pkl"

id = "tunnel-secondary-tertiary-casing"
filter {
    "all"
    new {
        "=="
        "brunnel"
        "tunnel"
    }
    new {
        "in"
        "class"
        "secondary"
        "tertiary"
    }
}
layout {
    `line-join` = "round"
}
paint {
    `line-color` = "#e9ac77"
    `line-opacity` = 1
    `line-width` {
        base = 1.2
        stops {
            new {
                8
                1.5
            }
            new {
                20
                17
            }
        }
    }
}